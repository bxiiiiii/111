int MaxValue(int N, int C, int[]v, int[]w)
{
    int dp[N][C+1];
    int i;

    for(i = 0; i <=C; i++)
        dp[0][i] = (i>=v[0] ? w[0]:0);

    for(i = 1; i < N; i++)
    {
        for(j = 0; j < C+1; j++)
        {
            int n = dp[i-1][j];
            int y = (j>=v[i] ? dp[i-1][j-v[j]]+w[i]:0);
        }
    }
    
    return dp[N-1][C];
}