#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define NUM 20
#define MAXSIZE 15 
int main()
{
	char* s[NUM], str[MAXSIZE];
	int i = 0,j = 0;
	do
	{
		fgets(str, MAXSIZE, stdin);   
		s[i]=(char*)malloc((strlen(str)+1)*sizeof(char));
		strcpy(s[i],str);         
		i++;
	} while(strcmp(str,"."));        
	for (; j < i; j++)
	{
		printf("%s\n", s[j]);	
	}	
	return 0;
}
